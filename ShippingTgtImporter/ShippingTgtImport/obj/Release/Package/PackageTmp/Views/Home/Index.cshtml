@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Kinross Plant Export Tool</h1>
    <p class="lead">Use this tool to export Kinross plant report to a readable plant file for Reconcilor 8</p>
    <h2>Select Plant File (*.xlsx / *.xls)</h2>
    @using (Html.BeginForm("Importexcel", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <table>
            <tr><td>Excel file</td><td><input type="file" id="FileUpload1" name="FileUpload1"  /></td></tr>
            <br />
            <tr><td></td><td><br /><input type="submit" id="Submit" name="Submit" value="Submit" class="btn btn-primary btn-lg" /></td></tr>
        </table>
    }
    @{ Html.RenderPartial("_Alerts"); }
    
</div>

