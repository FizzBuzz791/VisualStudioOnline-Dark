  <Code>
Function GetModelsXml(ByVal models as String, ByVal controlModel as Integer) as String
	If models.Trim.StartsWith("&lt;") Then
		dim controlModelString = String.Format("&lt;BlockModel id=""{0}""/&gt;", controlModel)
		models = models.Replace(controlModelString, "")
		Return models
	End If
	
	Dim modelList As String() = models.ToLower.Split(","c)
	Dim result as String = ""

	For Each model as String in modelList
		If model &lt;&gt; controlModel.ToString Then
			result += String.Format("&lt;BlockModel id='{0}'/&gt;", model)
		End If
	Next

	return result
End Function

Function GetControlModelName(ByVal modelString as String) As String
  If Not modelString.Contains(",") Then Return modelString
  return modelString.Split(",")(0).Trim()
End Function
