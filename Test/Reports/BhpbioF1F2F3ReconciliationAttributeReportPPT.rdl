        </Field>
        <Field Name="IsGeomet">
          <Value>=Fields!ProductSize.Value = "GEOMET"</Value>
        </Field>
        <Field Name="IsTonnes">
          <Value>=(Fields!Attribute.Value = "Tonnes")</Value>
        </Field>
        <Field Name="IsFactor">
          <Value>
            =(
            Fields!ReportTagId.Value = "F1Factor" OR
            Fields!ReportTagId.Value = "F15Factor" OR
            Fields!ReportTagId.Value  = "F2Factor" OR
            Fields!ReportTagId.Value  = "F3Factor" OR
            Fields!ReportTagId.Value  = "F25Factor" OR
            Fields!ReportTagId.Value  = "RFGM" OR
            Fields!ReportTagId.Value  = "RFMM" OR
            Fields!ReportTagId.Value  = "RFSTM"
            )
          </Value>
        </Field>
        <Field Name="IsRatio">
          <Value>
            =(
            Fields!IsFactor.Value Or (Fields!IsGeomet.Value And Not Fields!IsTonnes.Value)
            )
          </Value>
        </Field>
        <Field Name="IsNA">
          <Value>=(Not Fields!IsTonnes.Value) AndAlso (Fields!ReportTagId.Value = "F25Factor" Or Fields!ReportTagId.Value = "OreForRail")</Value>
        </Field>
        <Field Name="FactorSortOrder">
          <Value>
            =Switch(
            Fields!ReportTagId.Value = "GeologyModel",10,
            Fields!ReportTagId.Value = "ShortTermGeologyModel",20,
            Fields!ReportTagId.Value = "MiningModel",30,
            Fields!ReportTagId.Value = "GradeControlModel",40,
            Fields!ReportTagId.Value = "MineProductionExpitEqulivent",50,
            Fields!ReportTagId.Value = "OreForRail",60,
            Fields!ReportTagId.Value = "MiningModelOreForRailEquivalent",70,
