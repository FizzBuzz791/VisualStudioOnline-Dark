            Fields!Attribute.Value= "Fe", 1 ,
            Fields!Attribute.Value = "P", 2 ,
            Fields!Attribute.Value = "SiO2", 3 ,
            Fields!Attribute.Value = "Al2O3", 4 ,
            Fields!Attribute.Value = "LOI", 5,
            Fields!Attribute.Value = "Ultrafines", 6,
            true, 7)
          </Value>
