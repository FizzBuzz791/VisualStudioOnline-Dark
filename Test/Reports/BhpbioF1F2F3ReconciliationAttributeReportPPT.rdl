            Fields!ReportTagId.Value = "HubPostCrusherStockpileDelta",100,
            Fields!ReportTagId.Value = "PostCrusherStockpileDelta",110,
            Fields!ReportTagId.Value = "PortStockpileDelta",120,
            Fields!ReportTagId.Value = "MiningModelShippingEquivalent",130,
            Fields!ReportTagId.Value = "OreShipped",140,
            Fields!ReportTagId.Value = "F1Factor",150,
            Fields!ReportTagId.Value = "F15Factor",160,
            Fields!ReportTagId.Value = "RFGM",170,
            Fields!ReportTagId.Value = "RFMM",180,
            Fields!ReportTagId.Value = "RFSTM",190,
            Fields!ReportTagId.Value = "F2Factor",200,
            Fields!ReportTagId.Value = "F25Factor",210,
            Fields!ReportTagId.Value = "F3Factor",220,
            true, 999)
