  <Code>Public Function GetXmlAttributeAsCsv(ByVal xml As String, ByVal attribute As String, ByVal delimiter As String) As String
	Dim result As String = ""
	Dim index As Int32

	index = xml.IndexOf(attribute &amp; "=""")

	While index &gt; -1
		xml = xml.Substring(index + attribute.Length + 2)

		index = xml.IndexOf("""")

		result &amp;= xml.Substring(0, index) &amp; delimiter

		xml = xml.Substring(index)
		index = xml.IndexOf(attribute &amp; "=")
	End While

	Return result.TrimEnd(delimiter.ToCharArray())
End Function

Public Function ContainsCSVEntryWithoutText(ByVal csv As String, ByVal valueToCheck As String) As Boolean
            Dim containsEntry As Boolean = False

            If (Not String.IsNullOrEmpty(csv)) Then
                Dim entries As String() = csv.Split(New String() {","}, StringSplitOptions.RemoveEmptyEntries)
                For Each entry As String In entries
                    If (Not String.IsNullOrEmpty(entry)) Then
                        If (Not entry.Contains(valueToCheck)) Then
                            containsEntry = True
                        End If
                    End If
                Next
            End If

            Return containsEntry
        End Function</Code>
