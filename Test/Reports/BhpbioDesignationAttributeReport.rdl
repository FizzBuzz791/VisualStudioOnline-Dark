  <Code>
Public Shared Dim initialAttribute as String

Function SetValueOfInitialAttribute(ByVal datasetInitialAttribute as String)
	initialAttribute = datasetInitialAttribute 
End Function

Function DrawTopBorder(ByVal currentAttribute as String) as String

	Dim drawBorder as String 
	drawBorder = "false"

	If ( currentAttribute = initialAttribute) Then
		drawBorder = "true"
	End If

	return drawBorder 

End Function


Function CalculateGraphAxis(ByVal AxisRequest As String, ByVal FieldName As String, ByVal AttributePrecision As Double, ByVal MinValueString As String, ByVal MaxValueString As String) As Double
	Dim Diff As Double
	Dim LogDiff As Double
	Dim FloorLogDiff As Double
	Dim SigDiff As Double
	Dim SigMin As Double
	Dim MinMultiplier As Double
	Dim MinValue As Double
	Dim MaxValue AS Double
	
	Dim GraphStep As Double
	Dim GraphMin As Double
	Dim GraphMax As Double
	Dim closeToZero As Double = 0.000000000000000000000000000000000000000000000001
	
	Dim Result As Double

	If MinValueString Is Nothing Then
		MinValue = 0.0
	Else
		MinValue = CDbl(MinValueString)
	End If
	
	If MaxValueString Is Nothing Then
		MaxValue = 0.0
	Else
		MaxValue = CDbl(MaxValueString)
	End If
	
	If Not FieldName Is Nothing AndAlso (FieldName.tolower = "tonnes" Or FieldName.tolower = "volume") Then
		MaxValue =MaxValue / 1000
		MinValue =MinValue / 1000
	End If

	Diff = MaxValue - MinValue
	
	If Diff &gt; 0.0 Then
		LogDiff = Math.Log10(Diff)
		FloorLogDiff = Convert.ToInt32(Math.Floor(LogDiff))
		
		FloorLogDiff = FloorLogDiff - 1
		If FloorLogDiff &lt; AttributePrecision*-1 And AttributePrecision &gt; 0 Then
			FloorLogDiff = AttributePrecision*-1
		End If
		
		If MinValue &lt;&gt; 0 Then
			SigMin = Convert.ToInt32(Math.Floor(Math.Log10(Abs(MinValue))))
			SigMin = SigMin - 1
		Else
			SigMin = 0
		End If
		
		If SigMin &gt; 0 Then
			MinMultiplier = 10 ^ SigMin
		Else If SigMin &lt; 0 Then
			MinMultiplier = 1/(10^(Abs(SigMin)))
		Else 
			MinMultiplier = 1
		End If
		


		'SigDiff = 10 ^ (FloorLogDiff - 1)
		If FloorLogDiff &gt; 0 Then
			SigDiff = 10 ^ FloorLogDiff
		Else If FloorLogDiff &lt; 0 Then
			SigDiff = 1/(10^(Abs(FloorLogDiff)))
		Else 
			SigDiff = 1
		End If
		
		If Convert.ToInt32(Math.Floor(Diff / (1*SigDiff))) &lt;= 10 Then
			GraphStep = 1*SigDiff
		Else If Convert.ToInt32(Math.Floor(Diff / (5*SigDiff))) &lt;= 10 Then
			GraphStep = 5*SigDiff
		Else If Convert.ToInt32(Math.Floor(Diff / (10*SigDiff))) &lt;= 10 Then
			GraphStep = 10*SigDiff
		Else If Convert.ToInt32(Math.Floor(Diff / (25*SigDiff))) &lt;= 10 Then
			GraphStep = 25*SigDiff
		Else If Convert.ToInt32(Math.Floor(Diff / (50*SigDiff))) &lt;= 10 Then
			GraphStep = 50*SigDiff
		Else If Convert.ToInt32(Math.Floor(Diff / (100*SigDiff))) &gt;= 1 Then
			GraphStep = 100*SigDiff
		Else
			GraphStep = 1 * SigDiff
		End If
		
		GraphMin = (Convert.ToInt32(Math.Floor(MinValue / GraphStep)) - 2)*GraphStep
		If GraphMin &lt; 0 And MinValue &gt;= 0 Then
			GraphMin = 0
		End If
		
		GraphMax = GraphMin + GraphStep * (Convert.ToInt32((MaxValue - GraphMin) / GraphStep) + 2)
		
		If AxisRequest = "Step" Then
			Result = GraphStep
		Else If AxisRequest = "Min" Then
			Result = GraphMin
		Else If AxisRequest = "Max" Then
			Result = GraphMax
		Else
			Result = 0.0
		End If
	Else
		Result = Nothing
	End If
	
	CalculateGraphAxis = Result
End Function</Code>
