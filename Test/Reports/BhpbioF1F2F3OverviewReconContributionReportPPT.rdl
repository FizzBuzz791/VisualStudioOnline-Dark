  <Code>Function GetFactorResultIndex(ByVal relative As Double, IsAbsolute As Boolean, LowThreshold As Double, HighThreshold As Double) As String
    Return GetFactorResultIndex(relative, relative, IsAbsolute, LowThreshold, HighThreshold)
End Function

' This can be used with the 'Choose' statement in the SSRS expressions to make it easy
' to convert the index to a face image, color, etc
Function GetFactorResultIndex(ByVal relative As Double, ByVal absolute As Double, IsAbsolute As Boolean, LowThreshold As Double, HighThreshold As Double) As String
  Dim resultString as String = Nothing
  Dim resultIndex as Integer = Nothing

  If IsAbsolute And absolute = 0 Then
    resultString = "NoData"
  ElseIf Not IsAbsolute And relative = 0 Then
    resultString = "NoData"
  Else
    resultString = GetFactorResult(relative, absolute, IsAbsolute, LowThreshold, HighThreshold)
  End If
  
  Select Case resultString
    ' return indexes starting from 1, as this is what CHOOSE needs
    Case "NoData": resultIndex = 1
    Case "Low": resultIndex = 2
    Case "Medium": resultIndex = 3
    Case "High": resultIndex = 4
  End Select

  return resultIndex
End Function

Function GetFactorResult(ByVal relative As Double, ByVal absolute As Double, IsAbsolute As Boolean, LowThreshold As Double, HighThreshold As Double) As String
  
    Dim result As String

    If IsAbsolute Then
        If Math.Abs(absolute) &lt; LowThreshold Then
            result = "Low"
        ElseIf Math.Abs(absolute) &gt; LowThreshold And Math.Abs(absolute) &lt;  HighThreshold   Then
            result = "Medium"
        Else
            result = "High"
        End If
    Else

    LowThreshold = LowThreshold / 100.0
    HighThreshold = HighThreshold / 100.0

        If Math.Abs(1 - relative) &lt;  LowThreshold  Then
            result = "Low"
        ElseIf Math.Abs(1 - relative) &gt;  LowThreshold  And Math.Abs(1 - relative) &lt; HighThreshold  Then
            result = "Medium"
        Else
            result = "High"
        End If

    End If

    Return result
End Function

' Converts F15Factor to F1.5 and so on
Function TagIdToShortName(ByVal reportTagId as String) as String
  Return reportTagId.Replace("Factor", "").Replace("F15", "F1.5").Replace("F25", "F2.5")
End Function
