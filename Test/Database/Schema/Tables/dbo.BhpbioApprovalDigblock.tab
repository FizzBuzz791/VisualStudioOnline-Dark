IF OBJECT_ID('dbo.BhpbioApprovalDigblock') IS NOT NULL 
     DROP TABLE dbo.BhpbioApprovalDigblock
GO

CREATE TABLE dbo.BhpbioApprovalDigblock 
(
	DigblockId VARCHAR(31) COLLATE Database_Default NOT NULL,
	ApprovedMonth DATETIME NOT NULL,
	UserId INT NULL,
	SignoffDate DATETIME NULL,

	CONSTRAINT PK_BHPBIOAPPROVALDIGBLOCK PRIMARY KEY CLUSTERED
		(DigblockId, ApprovedMonth),
	CONSTRAINT FK_BHPBIOAPPROVALDIGBLOCK_DIGBLOCK FOREIGN KEY (DigblockId)
		REFERENCES dbo.Digblock (Digblock_Id),
	CONSTRAINT FK_BhpbioApprovalDigblock_User FOREIGN KEY (UserId)
		REFERENCES dbo.SecurityUser (UserId)
)
GO
